{
  "name": "Automata",
  "version": "0.0.0",
  "description": "Cadaver exquisito, #publicaciones #independientes, #",
  "main": "bin/www",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && tape tests/*-test.js | tap-spec",
    "sync": "browser-sync start --proxy 'localhost:3000' --files 'public'",
    "develop": "npm run sync & nodemon bin/www",
    "watch": "watchify src/main.js -o public/js/bundle.js",
    "scss": "scss --watch src/scss:public/css",
    "cpx": "cpx 'src/*.html' public -w",
    "start": "npm run scss & npm run watch & npm run cpx & nodemon bin/www",
    "db": "mongod"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hi-neron/Automata.git"
  },
  "keywords": [
    "cadaver",
    "exquisito",
    "unicauca",
    "dieño",
    "gráfico"
  ],
  "author": "Hi-Neron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hi-neron/Automata/issues"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.1",
    "express": "~4.13.4",
    "formidable": "^1.0.17",
    "fs-extra": "^0.30.0",
    "jquery": "^2.2.3",
    "jsonfile": "^2.3.0",
    "mongoose": "^4.4.14",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5",
    "string": "^3.3.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "cpx": "^1.3.1",
    "debug": "^2.2.0",
    "supertest": "^1.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  }
}
